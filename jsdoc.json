{
  "source": {
    "include": ["src-nest"],
    "includePattern": ".(t|j)s$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-tsimport-plugin/index.js"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs-web/"
  }
}